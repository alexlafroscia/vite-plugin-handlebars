{
  "name": "vite-plugin-handlebars",
  "version": "1.4.2",
  "description": "Vite plugin for Handlebars support in HTML",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "files": [
    "dist"
  ],
  "repository": "git@github.com:alexlafroscia/vite-plugin-handlebars.git",
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "microbundle -f es,cjs --target node --no-compress",
    "lint": "eslint .",
    "test": "jest",
    "release": "standard-version",
    "prepare": "husky install",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "handlebars": "^4.7.6"
  },
  "devDependencies": {
    "@movable/eslint-config": "^0.6.9",
    "@movable/eslint-config-typescript": "^0.2.1",
    "@movable/prettier-config": "^0.3.6",
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.20",
    "file-fixture-factory": "^1.0.1",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "microbundle": "^0.13.0",
    "standard-version": "^9.1.0",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.63"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{json,md,yml}": "prettier --write"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "volta": {
    "node": "14.15.4",
    "yarn": "1.22.10"
  }
}
